PROCEDURE "precounitario"(
    out result TABLE(
        CODIGOPRODUTO INTEGER,
        VALORTOTAL DECIMAL,
        QUANTIDADE INTEGER,
        PRECOUNITARIO DECIMAL
    )
)
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER
    READS SQL DATA AS
BEGIN
    result = SELECT PRODUTO_CODIGOPRODUTO AS CODIGOPRODUTO, 
    VALORTOTAL, QUANTIDADE, VALORTOTAL/QUANTIDADE as PrecoUnitario
    FROM PN_ITEMS_TRANSACAO
    WHERE QUANTIDADE >= 5; 
END